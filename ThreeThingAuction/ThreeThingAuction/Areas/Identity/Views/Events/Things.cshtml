@model IEnumerable<ThreeThingAuction.Models.Thing>

@{
    ViewData["Title"] = "Things";
    
}

<h2>Things</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Owned)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Owned)
                </td>
                <td></td>
            </tr>
        }
    </tbody>
</table>

<div>
    <a asp-action="Index">Back to Index</a>
    <a asp-action="CreateThing">Create thing</a>
    
</div>